pipeline {
    agent any


    stages {
        stage('Build') {
            steps {
                sh 'echo building...'
             }
        }
        stage('Stage II') {
            steps {
                sh 'echo "stage II Done..."'
                }
            }
            stage('Stage III Done') {
            steps {
                sh 'echo "stage III..."'
        }
    }
}
}